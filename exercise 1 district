import csv

def destricto(entrada):
    index = 0
    found= False
    listdico=-1
    for i in entrada:
        if found:
            listdico=[{"district":i[index]}]
            break
        for y in range(len(i)):
            if i[y] == 'DISTRITO':
                found=True             
                break
            index+=1
    for i in entrada:
        listdico.append({"district":i[index]})       
    return listdico    
    

csvarchivo = open('Agenda.csv',encoding="utf8",errors='ignore')
entrada = csv.reader(csvarchivo, delimiter=";")
listdico = destricto(entrada)
for i in listdico:
    for x,y in i.items():
        print(x+":"+y)
